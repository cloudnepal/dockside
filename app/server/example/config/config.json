    },
    "gateway": {
      "IP": "172.17.0.1",
      "enabled": false
    },
    "ide": {
        "path": "/opt/dockside",
        "command": [
            "/opt/dockside/launch.sh",
            "IDE_USER='{ideUser}'",
            "GIT_COMMITTER_NAME='{user.name}'",
            "GIT_COMMITTER_EMAIL='{user.email}'",
            "launch_ide"
        ]
    },
    "clientDistPath": "/home/newsnow/dockside/app/client/dist",
    "docker": {
        "bin": "/usr/bin/docker",
        "socket": "/var/run/docker.sock",
        "PATH": "/usr/bin:/bin",
        "HOME": "/home/newsnow",
        "security": {
            "apparmor": "unspecified",
            "seccomp": "unspecified"
        }
    },
    "lxcfs": {
        "mountpoints": [
            "cpuinfo",
            "diskstats",
            "meminfo",
            "stat",
            "swaps",
            "uptime"
        ],
        "available": false,
        "default": true
    }
